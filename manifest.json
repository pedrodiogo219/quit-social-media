{
    "manifest_version": 3,
    "name": "Quit social media",
    "description": "It's about time",
    "version": "1.0",
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "tabs",
        "webNavigation",
        "storage"
    ],
    "web_accessible_resources" : [
        {
            "resources": ["hello.html"],
            "matches": ["*://*/*"]
        }
    ]
}